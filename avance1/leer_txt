using System;
using System.IO;

class Program
{
    static void Main()
    {
        string path = @"C:\Users\ucb\Downloads\ijiji.txt";

        try
        {
            if (File.Exists(path))
            {
                using (StreamReader reader = new StreamReader(path))
                {
                    string line;
                    while ((line = reader.ReadLine()) != null)
                    {
                        Console.WriteLine(line);
                    }
                }
            }
        }
        catch (Exception ex)
        {
            Console.WriteLine("Ocurri√≥ un error al leer el archivo:");
            Console.WriteLine(ex.Message);
        }
    }
}
